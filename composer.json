{
    "name": "talbot-ninja/laravel-gcp-firestore",
    "description": "Laravel Database Driver for Google Cloud Firestore",
    "keywords": ["laravel", "database", "driver", "google", "firestore"],
    "homepage": "https://github.com/vtalbot/laravel-gcp-firestore",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Talbot",
            "email": "vincent.talbot@gmail.com",
            "homepage": "https://github.com/vtalbot",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "illuminate/database": "~6.0",
        "illuminate/support": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.3",
        "squizlabs/php_codesniffer": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "TalbotNinja\\LaravelGCPFirestore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TalbotNinja\\LaravelGCPFirestore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "TalbotNinja\\LaravelGCPFirestore\\FirestoreServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
